{
  "version": "0.0.0-development",
  "name": "@telus/semantic-release-config",
  "description": "semantic-release shareable config",
  "homepage": "https://github.com/telus/semantic-release-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/telus/semantic-release-config.git"
  },
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "telus",
    "semantic-release",
    "github",
    "npm"
  ],
  "engines": {
    "node": ">=8"
  },
  "files": [
    "github/*",
    "index.js"
  ],
  "bugs": {
    "url": "https://github.com/telus/semantic-release-config/issues"
  },
  "scripts": {
    "lint:ec": "npx editorconfig-checker .",
    "lint:js": "npx eslint .",
    "lint": "npx npm-run-all -p lint:*",
    "release:dryrun": "npx semantic-release --dry-run --branch $(git describe --contains --all HEAD)",
    "release": "npx semantic-release",
    "test": "echo \"No tests\""
  },
  "dependencies": {
    "@semantic-release/commit-analyzer": "^6.3.3",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^5.5.5",
    "@semantic-release/npm": "^5.3.5",
    "@semantic-release/release-notes-generator": "^7.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-angular": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@telus/eslint-config": "^2.0.1",
    "@telus/prettier-config": "^1.0.3",
    "@telus/remark-config": "^1.2.0",
    "editorconfig-checker": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.2.0",
    "eslint-plugin-jsx": "0.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^3.1.0"
  }
}
